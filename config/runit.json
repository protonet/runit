{

  "defaults": {
    "env": {
      "rails_env": "production",
      "rack_env":  "production"
    }
  },

  "services": {
    "rabbitmq": {
      "commands": {
        "start": "rabbitmq-server"
      },
      "env": {
        "rabbitmq_nodename": "protonet"
      },
      "options": {
        "setuidgid": "protonet:protonet",
        "root":      "/tmp"
      }
    },
    "apache2": {
      "commands": {
        "start": "apachectl -f /etc/apache2/apache2.conf -D FOREGROUND"
      },
      "options": {
        "setuidgid": "www-data:www-data"
      }
    }
  }

}
